<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fragmen.bwei.com.jdcase">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".net.App"
        android:allowBackup="true"
        android:icon="@drawable/jd_buy_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".view.ShowActivity" />
        <activity android:name=".view.HanderActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchActivity" />
        <activity android:name=".ShowSearActivity" />
        <activity android:name=".RemActivity" />
        <activity android:name=".OutLoginActivity"></activity>
    </application>

</manifest>